package my.mm.main;

import java.awt.Point;

import my.mm.generator.Block;
import my.mm.generator.BlockEntity;
import my.mm.generator.TileManager;
import my.mm.generator.World;
import my.mm.managers.LightManager;
import my.mm.managers.LightSource;


public class Check {
	
	public static boolean CollisionPlayerBlock(Point p1, Point p2){
		for(Block block : TileManager.blocks){
			if(block.isSolid()){
				if(block.contains(p1)||block.contains(p2)){
					return true;
				}
			}
		}
		return false;
	}
